    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Global Step Tracker
            window.currentStep = 1;
            
            const totalSteps = 7;
            const formTitle = document.getElementById('formTitle').querySelector('h1');
            const backBtn = document.getElementById('backBtn');
            const nextBtn = document.getElementById('nextBtn');
            const step7Buttons = document.getElementById('step7Buttons');
            const stepDots = Array.from(document.querySelectorAll('.progress-dot'));

            const formTitles = [
                'ATA form: Personal Details',
                'ATA form: Teaching Assignments',
                'ATA form: Administrative & Practicum Work',
                'ATA form: External Employment (Part-Time Only)',
                'ATA form: Remedial Module Assignments',
                'ATA form: Review & Submit',
                'ATA form: Admin Approval Section'
            ];

            // --- DISPLAY FUNCTION ---
            window.updateFormDisplay = function() {
                // 1. Hide all forms
                for(let i=1; i<=7; i++) {
                    const f = document.getElementById('form'+i);
                    if(f) f.style.display = 'none';
                }

                // 2. Show current form
                const currentForm = document.getElementById('form' + window.currentStep);
                if (currentForm) currentForm.style.display = 'block';

                // 3. Update Title
                if (formTitle) formTitle.textContent = formTitles[window.currentStep - 1];

                // 4. Update Dots (Visuals Only)
                stepDots.forEach((dot, index) => {
                    const stepNumber = index + 1;
                    dot.classList.remove('filled', 'active');
                    
                    if (window.isStepVisible && !window.isStepVisible(stepNumber)) {
                        dot.style.opacity = "0.3"; 
                        dot.style.cursor = "not-allowed";
                    } else {
                        dot.style.opacity = "1"; 
                        dot.style.cursor = "pointer";
                    }

                    if (stepNumber < window.currentStep) {
                        dot.textContent = '●'; dot.classList.add('filled');
                    } else if (stepNumber === window.currentStep) {
                        dot.textContent = '●'; dot.classList.add('active');
                    } else {
                        dot.textContent = '○';
                    }
                });

                // 5. Update Buttons dynamically based on roles
                if (backBtn) backBtn.textContent = (window.currentStep === 1) ? 'Return to Dashboard' : 'Back';

                let lastVisibleStep = 7;
                while(lastVisibleStep > 0 && window.isStepVisible && !window.isStepVisible(lastVisibleStep)) {
                    lastVisibleStep--;
                }

                if (nextBtn) {
                    nextBtn.textContent = (window.currentStep === lastVisibleStep) ? 'Submit' : 'Next';
                }

                if (step7Buttons) step7Buttons.style.display = (window.currentStep === 7) ? 'block' : 'none';

                // 6. Force Section Visibility Check
                if (window.handleInnerSectionVisibility) window.handleInnerSectionVisibility();

                // 7. Scroll top
                const container = document.querySelector('.form-cards-container');
                if (container) container.scrollTop = 0;
            };

            // --- DOT CLICK LISTENER ---
            stepDots.forEach((dot, index) => {
                dot.addEventListener('click', () => {
                    const targetStep = index + 1;
                    if (window.isStepVisible && !window.isStepVisible(targetStep)) {
                        alert("⚠️ This section is not applicable to your current role.");
                        return; 
                    }
                    window.currentStep = targetStep;
                    updateFormDisplay();
                    
                    // Trigger math calculation if jumping directly to Review Page
                    if (window.currentStep === 6 && window.calculateSummary) {
                        window.calculateSummary();
                    }
                });
            });

            // Back button logic
            if (backBtn) {
                backBtn.addEventListener('click', function() {
                    if (window.currentStep === 1) {
                        window.location.href = '/dashboard/window';
                    } else {
                        let prevStep = window.currentStep - 1;
                        while (prevStep > 0 && window.isStepVisible && !window.isStepVisible(prevStep)) {
                            prevStep--; 
                        }
                        if (prevStep > 0) {
                            window.currentStep = prevStep;
                            updateFormDisplay();
                        }
                    }
                });
            }

            // Initial Run
            updateFormDisplay();
        });
    </script>

    <script src="/js/ata_client.js"></script>


